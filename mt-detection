      "source": [
        "def precision(result, kind):\n",
        "  assert kind == 'p' or kind == 'n'\n",
        "  return result['t' + kind] / (result['t' + kind] + result['f' + kind])\n",
        "\n",
        "def recall(result, kind):\n",
        "  assert kind == 'p' or kind == 'n'\n",
        "  other = 'n' if kind == 'p' else 'p'\n",
        "  return result['t' + kind] / (result['t' + kind] + result['f' + other])\n",
        "\n",
        "def f1(result, kind):\n",
        "  p = precision(result, kind)\n",
        "  r = recall(result, kind)\n",
        "  return 2 * p * r / (p + r)\n",
        "\n",
        "print('Precision:', precision(result, 'p'))\n",
        "print('Recall:', recall(result, 'p'))\n",
        "f1p = f1(result, 'p')\n",
        "f1n = f1(result, 'n')\n",
        "print('Average F1 Score:', (f1p+f1n)/2)"
      ],
      "execution_count": 26,
