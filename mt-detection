      },
      "source": [
        "# construct DataFrame\n",
        "def file_to_df(filename):\n",
        "    with open(filename) as f:\n",
        "        lines = f.readlines()\n",
        "    \n",
        "    strip_elements = lambda arr: [elem.strip() for elem in arr]\n",
        "    def hm_label(label):\n",
        "        if label != 'H' and label != 'M':\n",
        "            raise Exception('label ' + str(label) + ' is not H or M')\n",
        "        return 0. if label == 'H' else 1.\n",
        "    train_dict = {\n",
        "        'Source': strip_elements(lines[::6]),\n",
        "        'Reference': strip_elements(lines[1::6]),\n",
        "        'Candidate': strip_elements(lines[2::6]),\n",
        "        'Bleu1': np.array(list(map(float, strip_elements(lines[3::6])))),\n",
        "        'Label': np.array(list(map(hm_label, strip_elements(lines[4::6])))),\n",
        "    }\n",
        "    return pd.DataFrame(train_dict)\n",
        "df = file_to_df('train.txt')\n",
        "df_test = file_to_df('test.txt')\n",
        "df.head()"
      ],
